{
    "manifest_version": 3,
    "name": "Codeforces Notes",
    "version": "1.0",
    "description": "Take notes on Codeforces questions",
    "permissions": [
      "cookies"
    ],
    "host_permissions": [
      "https://codeforces-notes.vercel.app/"
    ],
    "content_scripts": [
      {
        "matches":["https://codeforces.com/*/problem*"],
        "js":["problem.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
}