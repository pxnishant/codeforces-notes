{
    "manifest_version": 3,
    "name": "Codeforces Notes",
    "version": "1.0",
    "description": "Take notes on Codeforces questions",
    "permissions": [
      "identity",
      "storage"
    ],
    "content_scripts": [
      {
        "matches":["https://codeforces.com/*/problem*"],
        "js":["problem.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "oauth2": {
      "client_id": "373325692277-osd5votu77fnm158ajebi13bdse2f4l5.apps.googleusercontent.com",
      "scopes":["openid", "profile"]
    },
    "key" : ""  
  }